    {
      "linkId": "2",
      "text": "Symptoms for assessment",
      "type": "group",
      "item": [
        {
          "linkId": "2.1",
          "text": "Is there chest pain ?",
          "type": "boolean",
          "required": false,
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "ChestPain"
              }
            }
          ]
        },
        {
          "linkId": "2.2",
          "text": "Date of Onset of Chest Pain",
          "type": "dateTime",
          "required": false,
          "enableWhen": [
            {
              "question": "2.1",
              "answerBoolean": true
            }
          ],
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "ChestPainDate"
              }
            }
          ]
        },
        {
          "linkId": "2.3",
          "text": "Is there arm pain?",
          "type": "boolean",
          "required": false,
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "ArmPain"
              }
            }
          ]
        },
        {
          "linkId": "2.4",
          "text": "Date of Onset of Arm Pain",
          "type": "dateTime",
          "required": false,
          "enableWhen": [
            {
              "question": "2.3",
              "answerBoolean": true
            }
          ],
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "ArmPainDate"
              }
            }
          ]
        },
        {
          "linkId": "2.5",
          "text": "Is there jaw pain?",
          "type": "boolean",
          "required": false,
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "JawPain"
              }
            }
          ]
        },
        {
          "linkId": "2.6",
          "text": "Date of Onset of Jaw Pain",
          "type": "dateTime",
          "required": false,
          "enableWhen": [
            {
              "question": "2.5",
              "answerBoolean": true
            }
          ],
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "JawPainDate"
              }
            }
          ]
        },
        {
          "linkId": "2.7",
          "text": "Is the pain relieved with nitroglycerin or rest?",
          "type": "boolean",
          "required": false
        },
        {
          "linkId": "2.8",
          "text": "Is there shortness of breath?",
          "type": "boolean",
          "required": false,
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "ShortnessOfBreath"
              }
            }
          ]
        },
        {
          "linkId": "2.9",
          "text": "Date of Onset of Shortness of Breath",
          "type": "dateTime",
          "required": false,
          "enableWhen": [
            {
              "question": "2.8",
              "answerBoolean": true
            }
          ],
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "ShortnessOfBreathDate"
              }
            }
          ]
        }
      ]
    },
    {
      "linkId": "3",
      "text": "Clinical Assessment",
      "type": "group",
      "item": [
        {
          "linkId": "3.1",
          "text": "Is a decision made to perform cardiac catheterization or other angiography ?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "3.2",
          "text": "Is there substernal chest discomfort (generally described as pressure,heaviness, burning, or tightness) ?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "3.3",
          "text": "Is the chest pain  brought on by exertion or emotional stress and relieved by rest?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "3.4",
          "text": "Does the pain radiate to the left arm or jaw ?",
          "type": "boolean",
          "required": true
        }
      ]
    },
    {
      "linkId": "4",
      "text": "Assessment of Functional Capacity",
      "type": "group",
      "item": [
        {
          "linkId": "4.1",
          "text": "Can walk four blocks without stopping ?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "4.2",
          "text": "Can walk up a hill ?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "4.3",
          "text": "Can climb one flights of stairs without stopping ?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "4.4",
          "text": "Can perform heavy work around the house ?",
          "type": "boolean",
          "required": true
        }
      ]
    }
