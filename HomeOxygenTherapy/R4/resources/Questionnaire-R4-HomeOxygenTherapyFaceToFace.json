    {
      "linkId": "3",
      "type": "group",
      "text": "General questions",
      "item": [
        {
          "linkId": "3.1",
          "text": "Is this a face-to-face encounter for oxygen therapy? ",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".True"
              }
            }
          ]
        },
        {
          "linkId": "3.2",
          "text": "If No, purpose of the encounter: ",
          "type": "text",
          "required": false,
          "enableWhen": [
            {
              "question": "3.1",
              "operator": "=",
              "answerBoolean": false
            }
          ]
        },
        {
          "linkId": "3.3",
          "text": "Is this encounter an inpatient hospital stay?",
          "type": "boolean"
        },
        {
          "linkId": "3.4",
          "text": "Is patient currently mobile in their home? ",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".PatientMobile"
              }
            }
          ]
        },
        {
          "linkId": "3.5",
          "text": "If No and portable O2 is required, describe:  ",
          "type": "text",
          "required": false,
          "enableWhen": [
            {
              "question": "3.4",
              "operator": "=",
              "answerBoolean": false
            }
          ]
        },
        {
          "linkId": "3.6",
          "text": "Was blood gas study ordered and evaluated?",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".BloodGasOrderedAndEvaluated"
              }
            }
          ]
        },
        {
          "linkId": "3.7",
          "text": "If Yes, date of testing (MM/DD/YYYY):",
          "type": "date",
          "required": false,
          "enableWhen": [
            {
              "question": "3.6",
              "operator": "=",
              "answerBoolean": true
            }
          ],
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".BloodGasTestDate"
              }
            }
          ]
        },
        {
          "linkId": "3.8",
          "text": "Is patient in a chronic stable state?",
          "type": "boolean"
        },
        {
          "linkId": "3.9",
          "text": "describe:",
          "type": "text"
        },
        {
          "linkId": "3.10",
          "text": "Is there an expectation that home oxygen therapy will improve O2 Saturation?",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".True"
              }
            }
          ]
        },
        {
          "linkId": "3.11",
          "text": "describe:",
          "type": "text"
        },
        {
          "linkId": "3.12",
          "text": "Does patient live or travel to an area at high elevation?",
          "type": "boolean"
        },
        {
          "linkId": "3.13",
          "text": "If Yes, describe if relevant:",
          "type": "text",
          "required": false,
          "enableWhen": [
            {
              "question": "3.12",
              "operator": "=",
              "answerBoolean": true
            }
          ]
        },
        {
          "linkId": "3.14",
          "text": "Does patient have a reduced O2 carrying capacity?",
          "type": "boolean"
        },
        {
          "linkId": "3.15",
          "text": "If Yes, describe:",
          "type": "text",
          "required": false,
          "enableWhen": [
            {
              "question": "3.14",
              "operator": "=",
              "answerBoolean": true
            }
          ]
        }
      ]
    },
