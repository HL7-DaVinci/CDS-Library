    {
      "linkId": "10",
      "type": "group",
      "text": "Qualifying Criteria for Hospital Beds",
      "required": true,
      "item": [
        {
          "linkId": "10.1",
          "type": "group",
          "text": "Fixed Height Hospital Beds",
          "required": true,
          "item": [
            {
              "linkId": "10.1.1",
              "type": "boolean",
              "text": "Ordered",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"HospitalBedsAndAccessoriesPrepopulation\".FixedHeightBedsOrdered"
                  }
                }
              ]
            },
            {
              "linkId": "10.1.2",
              "type": "group",
              "text": "Qualification",
              "item": [
                {
                  "text": "Patient has a medical condition which requires positioning of the body in ways not feasible with an ordinary bed. (Elevation of the head/upper body less than 30 degrees does not usually require the use of hospital bed)",
                  "type": "boolean",
                  "linkId": "10.1.2.1"
                },
                {
                  "text": "The patient requires positioning of the body in ways not feasible with an ordinary bed to alleviate pain;",
                  "type": "boolean",
                  "linkId": "10.1.2.2"
                },
                {
                  "text": "Patient requires the head of the bed to be elevated more than 30 degrees most of the time due to congestive heart failure, chronic pulmonary disease, or problems with aspiration",
                  "type": "boolean",
                  "linkId": "10.1.2.3"
                },
                {
                  "text": "Patient requires traction equipment, which can only be attached to a hospital bed.",
                  "type": "boolean",
                  "linkId": "10.1.2.4"
                },
                {
                  "text": "Describe",
                  "type": "string",
                  "linkId": "11.1.2.5",
                  "required": true
                }
              ],
              "enableWhen": [
                {
                  "question": "10.1.1",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "10.2",
          "type": "group",
          "text": "Variable Height Hospital Beds",
          "required": true,
          "item": [
            {
              "linkId": "10.2.1",
              "type": "boolean",
              "text": "Ordered",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"HospitalBedsAndAccessoriesPrepopulation\".VariableHeightBedsOrdered"
                  }
                }
              ]
            },
            {
              "linkId": "10.2.2",
              "type": "group",
              "text": "Qualification",
              "required": true,
              "enableWhen": [
                {
                  "question": "10.2.1",
                  "operator": "=",
                  "answerBoolean": true
                }
              ],
              "item": [
                {
                  "linkId": "10.2.2.1",
                  "text": "Patient requires a bed height different than a fixed height hospital bed to permit transfers to chair, wheelchair, or standing position",
                  "type": "boolean"
                },
                {
                  "linkId": "10.2.2.2",
                  "text": "Describe",
                  "type": "text",
                  "required": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "10.3",
          "type": "group",
          "text": "Semi-electric Hospital Beds",
          "item": [
            {
              "linkId": "10.3.1",
              "type": "boolean",
              "text": "Ordered",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"HospitalBedsAndAccessoriesPrepopulation\".SemiElectricBedsOrdered"
                  }
                }
              ]
            },
            {
              "linkId": "10.3.2",
              "type": "group",
              "text": "Qualification",
              "enableWhen": [
                {
                  "question": "10.3.1",
                  "operator": "=",
                  "answerBoolean": true
                }
              ],
              "item": [
                {
                  "linkId": "11.0.2.1",
                  "text": "Patient requires frequent changes in body position and/or has an immediate need for a change in position. ",
                  "type": "boolean"
                },
                {
                  "linkId": "11.0.2.2",
                  "text": "Describe",
                  "type": "text",
                  "required": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "10.4",
          "type": "group",
          "text": "Trapeze Equipment",
          "required": true,
          "item": [
            {
              "linkId": "10.4.1",
              "type": "boolean",
              "text": "Ordered",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"HospitalBedsAndAccessoriesPrepopulation\".TrapezeOrdered"
                  }
                }
              ]
            },
            {
              "linkId": "10.4.2",
              "type": "group",
              "text": "Qualification",
              "required": true,
              "enableWhen": [
                {
                  "question": "10.4.1",
                  "operator": "=",
                  "answerBoolean": true
                }
              ],
              "item": [
                {
                  "linkId": "10.4.2.1",
                  "text": "Patient needs trapeze equipment to sit up because of a respiratory condition, to change body position for other medical reasons, or to get in or out of bed.",
                  "type": "boolean"
                },
                {
                  "linkId": "10.4.2.2",
                  "text": "Describe",
                  "type": "text",
                  "required": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "10.5",
          "type": "group",
          "text": "Bed Cradle",
          "required": true,
          "item": [
            {
              "linkId": "10.5.1",
              "type": "boolean",
              "text": "Ordered",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"HospitalBedsAndAccessoriesPrepopulation\".BedcradleOrdered"
                  }
                }
              ]
            },
            {
              "linkId": "10.5.2",
              "type": "group",
              "text": "Qualification",
              "enableWhen": [
                {
                  "question": "10.5.1",
                  "operator": "=",
                  "answerBoolean": true
                }
              ],
              "item": [
                {
                  "linkId": "10.5.2.1",
                  "text": "Patient needs a bed cradle to prevent contact with the bed coverings",
                  "type": "boolean"
                },
                {
                  "linkId": "10.5.2.2",
                  "text": "Describe",
                  "type": "text",
                  "required": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "10.6",
          "type": "group",
          "text": "Side Rails or Safety Enclosures",
          "required": true,
          "item": [
            {
              "linkId": "10.6.1",
              "type": "boolean",
              "text": "Ordered",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"HospitalBedsAndAccessoriesPrepopulation\".SafetyrailOrdered"
                  }
                }
              ]
            },
            {
              "linkId": "10.6.2",
              "type": "group",
              "text": "Qualification",
              "enableWhen": [
                {
                  "question": "10.6.1",
                  "operator": "=",
                  "answerBoolean": true
                }
              ],
              "item": [
                {
                  "linkId": "10.6.2.1",
                  "text": "Patient needs side rails on the hospital bed",
                  "type": "boolean"
                },
                {
                  "linkId": "10.6.2.2",
                  "text": "Describe",
                  "type": "text",
                  "required": true
                }
              ]
            }
          ]
        }
      ]
