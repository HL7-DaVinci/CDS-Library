              "enableBehavior": "all"
            },
            {
              "linkId": "14.1.4",
              "text": "Insulin treated times daily",
              "type": "integer",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
