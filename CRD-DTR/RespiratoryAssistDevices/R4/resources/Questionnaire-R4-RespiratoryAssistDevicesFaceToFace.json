    {
      "linkId": "6",
      "text": "Other qualifying observations",
      "type": "group",
      "required": true,
      "item": [
        {
          "linkId": "6.1",
          "text": "If patient has a neuromuscular disease only:",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".HasNeuromuscularDisease"
              }
            }
          ],
          "item": [
            {
              "linkId": "6.1.1",
              "text": "Maximal inspiratory pressure in units of cm H20:",
              "type": "decimal",
              "required": true,
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-unit",
                  "valueCoding": {
                    "display": "cm[H20]"
                  }
                },
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".MIP"
                  }
                }
              ],
              "enableWhen": [
                {
                  "question": "6.1",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            },
            {
              "linkId": "6.1.2",
              "text": "Forced vital capacity in % predicted:",
              "type": "decimal",
              "required": true,
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-unit",
                  "valueCoding": {
                    "display": "%"
                  }
                },
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".FVC"
                  }
                }
              ],
              "enableWhen": [
                {
                  "question": "6.1",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "6.2",
          "text": "If patient has severe COPD:",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".HasCOPD"
              }
            }
          ],
          "item":[
            {
              "linkId": "6.2.1",
              "text": "OSA and treatment with a CPAP was considered and ruled out:",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-itemControl",
                  "valueCodeableConcept": {
                    "coding": [
                      {
                        "code": "radio-button",
                        "system": "http://hl7.org/fhir/questionnaire-item-control"
                      }
                    ]
                  }
                }
              ],
              "type": "choice",
              "answerValueSet": "http://terminology.hl7.org/ValueSet/v2-0136",
              "enableWhen": [
                {
                  "question": "6.2",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            },
            {
              "linkId": "6.2.2",
              "text": "Describe:",
              "type": "string",
              "required": true,
              "enableWhen": [
                {
                  "question": "6.2",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "6.3",
          "text": "If patient has CSA or CompSA:",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".HasCSAorCompSA"
              }
            }
          ],
          "item":[
            {
              "linkId": "6.3.1",
              "text": "Was significant improvement of sleep associated hypoventilation with use of an E0470 or E0471 while breathing the usual FIO2?",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-itemControl",
                  "valueCodeableConcept": {
                    "coding": [
                      {
                        "code": "radio-button",
                        "system": "http://hl7.org/fhir/questionnaire-item-control"
                      }
                    ]
                  }
                }
              ],
              "type": "choice",
              "required": true,
              "answerValueSet": "http://terminology.hl7.org/ValueSet/v2-0136",
              "enableWhen": [
                {
                  "question": "6.3",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            },
            {
              "linkId": "6.3.2",
              "text": "Describe:",
              "type": "string",
              "required": true,
              "enableWhen": [
                {
                  "question": "6.3",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "6.4",
          "text": "If patient has Hypoventilation Syndrome, provide Spirometry results:",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".HasHypoventilationDisorder"
              }
            }
          ],
          "item": [
            {
              "linkId": "6.4.1",
              "text": "FEV1/FVC (%):",
              "type": "decimal",
              "required": true,
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-unit",
                  "valueCoding": {
                    "display": "%"
                  }
                },
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".FEV1_FVC_Ratio"
                  }
                }
              ],
              "enableWhen": [
                {
                  "question": "6.4",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            },
            {
              "linkId": "6.4.2",
              "text": "FEV1 (% predicted):",
              "required": true,
              "type": "decimal",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-unit",
                  "valueCoding": {
                    "display": "%"
                  }
                },
                {
                  "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".FEV1"
                  }
                }
              ],
              "code": [
                {
                  "code": "20152-5",
                  "display": "FEV1 measured/predicted",
                  "system": "http://loinc.org"
                }
              ],
              "enableWhen": [
                {
                  "question": "6.4",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            }
          ]
        },
        {
          "linkId": "6.5",
          "text": "If patient has Obstructive Sleep Apnea:",
          "type": "boolean",
          "extension": [
            {
              "url": "http://hl7.org/fhir/uv/sdc/StructureDefinition/sdc-questionnaire-initialExpression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"RespiratoryAssistDevicesFaceToFacePrepopulation\".HasOSA"
              }
            }
          ],
          "item":[
            {
              "linkId": "6.5.1",
              "text": "Was E0601 (CPAP) tried and not effective on therapeutic trial conducted in a facility or home setting?",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-itemControl",
                  "valueCodeableConcept": {
                    "coding": [
                      {
                        "code": "radio-button",
                        "system": "http://hl7.org/fhir/questionnaire-item-control"
                      }
                    ]
                  }
                }
              ],
              "type": "choice",
              "required": true,
              "answerValueSet": "http://terminology.hl7.org/ValueSet/v2-0136",
              "item":[
                {
                  "linkId": "6.5.1.1",
                  "text": "In which setting was E0601 (CPAP) tried on therapeutic trial conducted?",
                  "type": "choice",
                  "required": true,
                  "answerOption": [
                    {
                      "valueCoding": {
                        "display": "facility"
                      }
                    },
                    {
                      "valueCoding": {
                        "display": "home"
                      }
                    }
                  ],
                  "enableWhen": [
                    {
                      "question": "6.5.1",
                      "operator": "=",
                      "answerCoding": {
                        "code": "Y",
                        "system": "http://terminology.hl7.org/CodeSystem/v2-0136"
                      }
                    },
                    {
                      "question": "6.5.1",
                      "operator": "=",
                      "answerCoding": {
                        "code": "N",
                        "system": "http://terminology.hl7.org/CodeSystem/v2-0136"
                      }
                    }
                  ]
                }
              ],
              "enableWhen": [
                {
                  "question": "6.5",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            },
            {
              "linkId": "6.5.2",
              "text": "E0601 patient is not tolerating the therapy at current settings and lower pressures settings were tried and failed to:",
              "type": "choice",
              "required": true,
              "answerOption": [
                {
                  "valueCoding": {
                    "display": "adequately control the symptoms of OSA"
                  }
                },
                {
                  "valueCoding": {
                    "display": "improve sleep quality"
                  }
                },
                {
                  "valueCoding": {
                    "display": "reduce the AHI/RDI to acceptable levels"
                  }
                }
              ],
              "enableWhen": [
                {
                  "question": "6.5",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            },
            {
              "linkId": "6.5.3",
              "text": "Note: E0471 is not covered for diagnosis of OSA",
              "type": "display",
              "enableWhen": [
                {
                  "question": "6.5",
                  "operator": "=",
                  "answerBoolean": true
                }
              ]
            }
          ]
        }
      ]
