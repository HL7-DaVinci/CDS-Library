    {
      "linkId": "8",
      "type": "group",
      "text": "Other treatments",
      "item": [
        {
          "linkId": "8.1",
          "type": "boolean",
          "text": "Have treatments for respiratory-related symptoms been tried in the past?"
        },
        {
          "linkId": "8.2",
          "type": "group",
          "text": "Details",
          "required": true,
          "item": [
            {
              "linkId": "8.2.1",
              "type": "group",
              "text": "If Yes: Indicate the treatments that have been tried:",
              "item": [
                {
                  "linkId": "8.2.1.1",
                  "type": "open-choice",
                  "text": "Bronchodilators",
                  "answerOption": [
                    {
                      "valueCoding": {
                        "code": "Short-Acting"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "Long-Acting"
                      }
                    }
                  ],
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-answerRepeats",
                      "valueBoolean": true
                    }
                  ]
                },
                {
                  "linkId": "8.2.1.2",
                  "type": "open-choice",
                  "text": "Steroids",
                  "answerOption": [
                    {
                      "valueCoding": {
                        "code": "IV"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "PO"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "Inhaled"
                      }
                    }
                  ],
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-answerRepeats",
                      "valueBoolean": true
                    }
                  ]
                },
                {
                  "linkId": "8.2.1.3",
                  "type": "open-choice",
                  "text": "Ventilation",
                  "answerOption": [
                    {
                      "valueCoding": {
                        "code": "Non-Invasive"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "Invasive"
                      }
                    }
                  ],
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-answerRepeats",
                      "valueBoolean": true
                    }
                  ]
                },
                {
                  "linkId": "8.2.1.4",
                  "type": "open-choice",
                  "text": "Anti-Inflammatory",
                  "answerOption": [
                    {
                      "valueCoding": {
                        "code": "Cromolyn Sodium"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "Interleukin(IL)-10"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "Interferon-gamma"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "Specific PDE4-inhibitors (ciolomilast and roflumilast)"
                      }
                    }
                  ],
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-answerRepeats",
                      "valueBoolean": true
                    }
                  ]
                },
                {
                  "linkId": "8.2.1.5",
                  "type": "open-choice",
                  "text": "Other",
                  "answerOption": [
                    {
                      "valueCoding": {
                        "code": "Antibiotics"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "Surfactin"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "PAP/CPAP"
                      }
                    },
                    {
                      "valueCoding": {
                        "code": "Pulmonary Rehabilitation"
                      }
                    }
                  ],
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-answerRepeats",
                      "valueBoolean": true
                    }
                  ]
                }
              ]
            },
            {
              "linkId": "8.2.2",
              "type": "open-choice",
              "text": "Indicate effectiveness for the above treatments",
              "answerOption": [
                {
                  "valueCoding": {
                    "code": "No improvement"
                  }
                },
                {
                  "valueCoding": {
                    "code": "Limited improvement"
                  }
                },
                {
                  "valueCoding": {
                    "code": "Some improvement"
                  }
                },
                {
                  "valueCoding": {
                    "code": "Able to avoid O2 except with exercise"
                  }
                },
                {
                  "valueCoding": {
                    "code": "Requires nocturnal O2 only"
                  }
                },
                {
                  "valueCoding": {
                    "code": "Other",
                    "type": "text"
                  }
                }
              ]
            }
          ],
          "enableWhen": [
            {
              "question": "8.1",
              "operator": "=",
              "answerBoolean": true
            }
          ]
        },
        {
          "linkId": "8.3",
          "type": "boolean",
          "text": "If No: Were any of the above treatments considered?",
          "required": true,
          "enableWhen": [
