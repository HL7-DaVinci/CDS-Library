    {
      "linkId": "3",
      "type": "group",
      "text": "General questions",
      "item": [
        {
          "linkId": "3.1",
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/sub-questionnaire",
              "valueCanonical": "questionnaire/encounter"
            },
            {
              "url": "http://hl7.org/fhir/StructureDefinition/sub-questionnaire-expand",
              "valueBoolean": true
            }
          ],
          "type": "display",
          "text": "This is a placeholder for Encounter"
        },
        {
          "linkId": "3.3",
          "text": "Is this a face-to-face encounter for oxygen therapy?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "3.3.1",
          "text": "If No, purpose of the encounter:  ",
          "type": "text",
          "required": true,
          "enableWhen": [
            {
              "question": "3.3",
              "operator": "=",
              "answerBoolean": false
            }
          ]
        },
        {
          "linkId": "3.4",
          "text": "Is this encounter an inpatient hospital stay?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "3.5",
          "text": "Is patient currently mobile in their home?",
          "type": "boolean",
          "required": true,
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".PatientMobile"
              }
            }
          ]
        },
        {
          "linkId": "3.5.1",
          "text": "If No, is portable O2 is required?",
          "type": "boolean",
          "required": true,
          "enableWhen": [
            {
              "question": "3.5",
              "operator": "=",
              "answerBoolean": false
            }
          ]
        },
        {
          "linkId": "3.5.2",
          "text": "If yes, describe why:",
          "type": "text",
          "required": true,
          "enableWhen": [
            {
              "question": "3.5.1",
              "operator": "=",
              "answerBoolean": true
            }
          ]
        },
        {
          "linkId": "3.6",
          "text": "Was arterial blood gas study ordered and evaluated?",
          "type": "boolean",
          "required": true,
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".BloodGasOrderedAndEvaluated"
              }
            }
          ]
        },
        {
          "linkId": "3.6.1",
          "text": "If Yes, what was the date of testing (MM/DD/YYYY):",
          "type": "date",
          "required": true,
          "enableWhen": [
            {
              "question": "3.6",
              "operator": "=",
              "answerBoolean": true
            }
          ],
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".BloodGasTestDate"
              }
            }
          ]
        },
        {
          "linkId": "3.7",
          "text": "Is patient in a chronic stable state?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "3.7.1",
          "text": "describe:",
          "type": "text",
          "required": true
        },
        {
          "linkId": "3.8",
          "text": "Is there an expectation that home oxygen therapy will improve O2 Saturation?",
          "type": "boolean",
          "required": true,
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"HomeOxygenTherapyPrepopulation\".True"
              }
            }
          ]
        },
        {
          "linkId": "3.8.1",
          "text": "describe:",
          "type": "text",
          "required": true
        },
        {
          "linkId": "3.9",
          "text": "Does patient live or travel to an area at high elevation?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "3.9.1",
          "text": "If Yes, describe:",
          "type": "text",
          "required": true,
          "enableWhen": [
            {
              "question": "3.9",
              "operator": "=",
              "answerBoolean": true
            }
          ]
        },
        {
          "linkId": "3.10",
          "text": "Does patient have a reduced O2 carrying capacity?",
          "type": "boolean",
          "required": true
        },
        {
          "linkId": "3.10.1",
          "text": "If Yes, describe:",
          "type": "text",
          "required": true,
          "enableWhen": [
            {
              "question": "3.10",
              "operator": "=",
              "answerBoolean": true
            }
          ]
        }
      ]
    },
    {
      "linkId": "4",
      "type": "group",
      "text": "Primary diagnosis (to support need for home oxygen):",
      "required": true,
      "item": [
        {
          "linkId": "4.1",
          "text": "Diagnoses",
          "type": "group",
          "item": [
            {
              "type": "open-choice",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"HomeOxygenTherapyPrepopulation\".RelevantDiagnoses"
                  }
                }
              ],
              "required": false,
              "linkId": "4.1.1",
              "text": "End Stage / Severe Lung Disease",
              "repeats": true,
              "answerOption": []
            },
            {
              "linkId": "4.1.2",
              "text": "Other Diagnoses",
              "type": "open-choice",
              "required": true,
              "repeats": true,
              "answerOption": [],
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
                  "valueExpression": {
                    "language": "text/cql",
                    "expression": "\"HomeOxygenTherapyPrepopulation\".OtherDiagnoses"
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "group",
          "required": false,
          "linkId": "4.2",
          "text": "Hypoxemia (expected to improve with oxygen therapy) and supporting evidence",
          "item": [
            {
              "type": "group",
              "required": false,
              "linkId": "4.2.1",
              "text": "Pulmonary hypertension / Cor Pulmonale",
              "item": [
                {
                  "type": "integer",
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
                      "valueExpression": {
                        "language": "text/cql",
                        "expression": "\"HomeOxygenTherapyPrepopulation\".PulmonaryArteryPressure"
                      }
                    }
                  ],
                  "required": false,
                  "linkId": "4.2.1.1",
                  "text": "Pulmonary arterial Pressure (PaP): Result:"
                }
              ]
            },
            {
              "type": "open-choice",
              "repeats": true,
              "required": false,
              "linkId": "4.2.2",
              "text": "Recurring heart failure secondary to chronic Cor Pulmonale",
              "answerOption": [
                {
                  "valueCoding": {
                    "display": "Edema"
                  }
                },
                {
                  "valueCoding": {
                    "display": "Pulmonary Rales"
                  }
                },
                {
                  "valueCoding": {
                    "display": "Fluid on CXR"
                  }
                },
                {
                  "valueCoding": {
                    "display": "Gated blood pool scan"
                  }
                },
                {
                  "valueCoding": {
                    "display": "“p” Pulmonale on ECG"
                  }
                },
                {
                  "valueCoding": {
                    "display": "Echocardiogram"
                  }
                }
              ]
            },
            {
              "type": "group",
              "extension": [
                {
                  "url": "http://hl7.org/fhir/StructureDefinition/questionnaire-answerRepeats",
                  "valueBoolean": true
                }
              ],
              "required": false,
              "linkId": "4.2.3",
              "text": "Erythrocythemia",
              "answerOption": [
                {
                  "valueCoding": {
                    "display": "Erythrocytosis"
                  }
                },
                {
                  "valueCoding": {
                    "display": "Hematocrit/hemoglobin result (Hct/Hgb):"
                  }
                }
              ],
              "item": [
                {
                  "type": "boolean",
                  "required": false,
                  "linkId": "4.2.3.1",
                  "text": "Erythrocytosis"
                },
                {
                  "type": "integer",
                  "required": false,
                  "linkId": "4.2.3.2",
                  "text": "Hematocrit/hemoglobin result (Hct/Hgb):",
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
                      "valueExpression": {
                        "language": "text/cql",
                        "expression": "\"HomeOxygenTherapyPrepopulation\".HematocritThatIsGreaterThanThreshold"
                      }
                    }
                  ]
                }
              ]
            },
            {
              "type": "boolean",
              "required": false,
              "linkId": "4.2.4",
              "text": "Cognitive impairment",
              "item": [
                {
                  "type": "boolean",
                  "required": false,
                  "linkId": "4.2.4.1",
                  "text": "Mental status exam"
                }
              ]
            },
            {
              "type": "boolean",
              "required": false,
              "linkId": "4.2.5",
              "text": "Nocturnal restlessness, morning headache, and/or daytime somnolence",
              "item": [
                {
                  "type": "boolean",
                  "required": false,
                  "linkId": "4.2.5.1",
                  "text": "Nocturnal oximetry"
                },
                {
                  "type": "boolean",
                  "required": false,
                  "linkId": "4.2.5.2",
                  "text": "Sleep Study"
                }
              ]
            },
            {
              "type": "group",
              "linkId": "4.2.6",
              "text": "Exertional hypoxia / desaturation",
              "item": [
                {
                  "type": "string",
                  "required": false,
                  "linkId": "4.2.6.1",
                  "text": "Value",
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
                      "valueExpression": {
                        "language": "text/cql",
                        "expression": "\"HomeOxygenTherapyPrepopulation\".ArterialOxygenSaturationExercise"
                      }
                    }
                  ]
                },
                {
                  "type": "boolean",
                  "required": false,
                  "linkId": "4.2.6.2",
                  "text": "O2 test",
                  "extension": [
                    {
                      "url": "http://hl7.org/fhir/StructureDefinition/cqf-expression",
                      "valueExpression": {
                        "language": "text/cql",
                        "expression": "\"HomeOxygenTherapyPrepopulation\".IsArterialOxygenSaturationExerciseTested"
                      }
                    }
                  ]
                },
                {
                  "type": "boolean",
                  "required": false,
                  "linkId": "4.2.6.3",
                  "text": "“6” minute walk (optional)"
                }
              ]
            },
            {
              "type": "string",
              "required": false,
              "linkId": "4.2.7",
              "text": "Clinical trials",
              "item": [
                {
                  "type": "boolean",
                  "required": false,
                  "linkId": "4.2.7.1",
                  "text": "Long-term O2 therapy"
                },
                {
                  "type": "boolean",
                  "required": false,
                  "linkId": "4.2.7.2",
                  "text": "Cluster headache"
                }
              ]
            }
          ]
        }
      ]
